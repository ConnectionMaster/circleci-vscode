{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2019",
    "lib": [
      "ES7",
      "ES6",
      "ES2019",
      "ES2020",
      "ES2020.String"
    ],
    "outDir": "dist",
    "rootDir": ".",
    "sourceMap": false,
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node"
  },
  "exclude": [
    "node_modules",
    ".vscode-test"
  ]
}
